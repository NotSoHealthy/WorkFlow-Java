<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="Hbox" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="80.0" stylesheets="@css/ItemsInscriptions.css">
         <children>
            <Label fx:id="TxtDateInscrit" alignment="CENTER" text="Date Inscription" />
            <ComboBox fx:id="comboBox" onAction="#comboBoxChange" prefWidth="120.0" />
            <Label fx:id="TxtFormation" alignment="CENTER" text="Formation" />
            <Label fx:id="TxtEmploye" alignment="CENTER" text="Employe" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
      <Pane prefHeight="10.0" prefWidth="200.0" />
   </children>
</VBox>
